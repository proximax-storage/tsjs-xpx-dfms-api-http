  "devDependencies": {
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.16",
    "@types/tar-stream": "^1.6.1",
    "chai": "^4.2.0",
    "cids": "^0.7.1",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.1",
